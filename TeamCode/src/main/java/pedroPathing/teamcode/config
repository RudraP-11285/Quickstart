Expansion Hub:
    Motor:
        IGNORE RN // 0 - verticalRight
        IGNORE RN // 1 - verticalLeft
        REPLACEMENT PORT // 1 - verticalRight
        REPLACEMENT PORT // 2 - verticalLeft
        REPLACEMENT PORT // 0 - leftRear
        3 - horizontalDrive
    Servo:
        0 - deposClaw
        1 - indicator_servo
    Analog Input Devices:
        0 - depositEncoder1
        1 - depositEncoder2

Control Hub:
    Motor:
        0 - leftFront
        1 - rightFront
        2 - rightRear
        IGNORE RN // 3 - leftRear
    Servo:
        0 - deposExtendo
        1 - deposArm
        2 - intakeArm
        3 - intakeClaw
        4 - intakeRotate
        5 - intakeWrist
    Digital Devices:
        0 - magLimVertical1
        1 - magLimVertical2
        6 - magLimHorizontal1
        7 - magLimHorizontal2
    Analog Input Devices:
        0 - wristEncoder1
        1 - wristEncoder2
        2 - armEncoder1
        3 - armEncoder2
    I2C Bus 1:
        0 - colorsensor
    I2C Bus 2:
        0 -
